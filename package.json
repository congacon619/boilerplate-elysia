{
  "name": "app",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "start": "NODE_ENV=production bun run ./src/index.ts",
    "start:prod": "bun build \\\n\t--compile \\\n\t--minify-whitespace \\\n\t--minify-syntax \\\n\t--target bun \\\n\t--outfile server \\\n\t./src/index.ts && ./server",
    "----CHECK": "echo CHECK",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "----DATABASE": "",
    "db:generate": "bun prisma generate --schema prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy",
    "db:migrate": "prisma migrate dev --create-only --name init",
    "----EMAIL": "",
    "email": "email dev --dir src/emails"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.8",
    "@elysiajs/bearer": "^1.2.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/opentelemetry": "^1.2.0",
    "@elysiajs/server-timing": "^1.2.1",
    "@elysiajs/swagger": "^1.2.2",
    "@keyv/redis": "^4.3.2",
    "@prisma/client": "^6.5.0",
    "@react-email/components": "0.0.34",
    "@sinclair/typebox": "^0.34.31",
    "bullmq": "^5.45.2",
    "dayjs": "^1.11.13",
    "elysia": "latest",
    "elysia-i18next": "^4.0.3",
    "elysia-rate-limit": "^4.3.0",
    "elysia-xss": "^1.0.3",
    "elysiajs-helmet": "^1.0.2",
    "i18next": "^24.2.3",
    "ioredis": "^5.6.0",
    "itty-time": "^1.0.6",
    "jose": "^6.0.10",
    "keyv": "^5.3.2",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "mitt": "^3.0.1",
    "nanoid": "^5.1.5",
    "node-telegram-bot-api": "^0.66.0",
    "otplib": "^12.0.1",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^3.1.0",
    "range_check": "^3.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis-semaphore": "^5.6.2",
    "resend": "^4.2.0",
    "svg-captcha": "^1.4.0",
    "type-fest": "^4.38.0",
    "ua-parser-js": "^2.0.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/lodash": "^4.17.16",
    "@types/mime-types": "^2.1.4",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/react": "^19.0.12",
    "bun-types": "latest",
    "prisma": "^6.5.0",
    "react-email": "4.0.2"
  },
  "module": "src/index.js"
}
